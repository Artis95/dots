    "vim.statusBarColors": {
        "normal": "#b16286",
        "insert": "#d79921",
        "visual": "#458588",
        "visualline": "#2aa198",
        "visualblock": "#859900",
        "replace": "#b58900"
    },
    "workbench.colorCustomizations": {
        "editor.background": "#151515",
        "editor.lineHighlightBackground": "#202020",
        "editorSuggestWidget.background": "#151515",
        "editorSuggestWidget.highlightForeground": "#b16286",
        "editorSuggestWidget.selectedBackground": "#303030",
        "statusBar.background": "#b16286",
        "sideBar.background": "#151515",
        "tab.inactiveBackground": "#202020",
        "tab.activeBackground": "#151515",
